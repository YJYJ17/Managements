<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="https://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/listeditbox"/>
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:top="2dp"
        android:left="2dp"
        android:right="2dp"
    >
        <shape>
            <solid android:color="@color/transparency"/>
            <stroke
                android:width="0.5dp"
                android:color="@color/listeditbox"/>
        </shape>
    </item>
</layer-list>